def_key "h"
  jump_to_parent_directory

def_key "H"
  previous_column

def_key "k"
  scroll_up

def_key "j"
  scroll_down

def_key "l"
  enter_directory

def_key "l"
  play_item

def_key "l"
  add_item_to_playlist

def_key "L"
  next_column

def_key "K"
  page_up

def_key "J"
  page_down

def_key "space"
  pause

def_key "g"
  move_home

def_key "G"
  move_end

def_key "d"
  delete_playlist_items

def_key "L"
  show_lyrics
